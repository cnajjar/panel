{
    "name":"contao-bootstrap/panel",
    "description":"Bootstrap panel component for Contao CMS",
    "keywords":["bootstrap", "components", "contao", "panel"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"https://netzmacht.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/contao-bootstrap/panel/issues",
        "wiki":"http://contao-bootstrap.netzmacht.de",
        "source":"https://github.com/contao-bootstrap/panel"
    },
    "require":{
        "php":">=7.1",
        "contao/core-bundle":"^4.4",
        "contao-bootstrap/core": "^2.0",
        "contao-community-alliance/meta-palettes":"^1.11 || ^2.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "phpcq/all-tasks": "^1.2"
    },
    "autoload": {
        "psr-4": { "ContaoBootstrap\\Panel\\": "src/" }
    },
    "extra":{
        "contao": {
            "transifex": {
                "project": "contao-bootstrap",
                "prefix": "panel-",
                "languages_cto": "src/Resources/contao/languages",
                "languages_tx": ".tx"
            }
        },
        "contao-manager-plugin": "ContaoBootstrap\\Panel\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-develop": "2.1.x-dev",
            "dev-support/1.0.x": "1.0.x-dev"
        }
    }
}
